{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent"
        ]
      }
    ],
    "css-prefix-order": "off",
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 118",
    "not firefox <= 118",
    "not safari <= 17",
    "not and_chr <= 117",
    "not chrome <= 118",
    "not edge <= 118",
    "not ios_saf <= 17",
    "not opera <= 103",
    "not samsung <= 22"
  ]
}